 <section class="shop_section layout_padding">
    <div class="container">
      <div class="heading_container heading_center">
        <h2>
          Latest Watches
        </h2>
      </div>
      <div class="row">
        
        
        {{#each products}}
        <div class="col-sm-6 col-xl-3">
          <div class="box">
            <a href="/product-page?id={{this._id}}">
              <div class="img-box">
                <img src="/product-images/{{this._id}}.jpg" alt="">
              </div>
              <div class="detail-box">
                <h6>
                  {{this.name}}
                </h6>
                
                <h6>
                  Price:
                  <span>
                    ${{this.price}}
                  </span>
                </h6>
              </div>
              <div class="new">
                <span>
                  New
                </span>
              </div>
            </a>
          </div>
        </div>
        {{/each}}
       
    
        
      </div>
     
    </div>
  </section>
